{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env.local",
    "dev:staging": "NODE_ENV=staging nuxt dev --dotenv .env.staging",
    "build:staging": "NODE_ENV=staging nuxt build --dotenv .env.staging",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:production": "NODE_ENV=production nuxt build",
    "lint": "eslint . --ext .vue,.js,.ts",
    "lint:fix": "eslint . --ext .vue,.js,.ts --fix",
    "prettier:check": "prettier --check \"**/*.{vue,js,ts}\"",
    "prettier:fix": "prettier --write \"**/*.{vue,js,ts}\""
  },
  "dependencies": {
    "@nuxt/eslint": "^1.3.0",
    "@provetcloud/css": "^1.1.0",
    "@provetcloud/web-components": "^1.3.4",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@provetcloud/web-components-vue-types": "^1.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vue-tsc": "^2.2.10"
  }
}